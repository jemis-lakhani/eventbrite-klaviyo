{
  "version": 2,
  "functions": {
   "api/webhook.js": {
      "runtime": "nodejs16.x"
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/api/attendee-updated",
      "dest": "/api/attendee-updated.js"
    },
    {
      "src": "/api/order-updated",
      "dest": "/api/order-updated.js"
    },
    {
      "src": "/(.*)",
      "dest": "/bin/www"
    }
  ]
}