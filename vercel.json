{
  "version": 2,
  "builds": [
    {
      "src": "bin/www",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["config/**", "helpers/**", "controllers/**", "routes/**", "app.js"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bin/www"
    }
  ]
}
